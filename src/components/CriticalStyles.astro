<style is:global>
  /* FONT FACES */
  @font-face {
    font-display: swap;
    font-family: onvan;
    font-style: normal;
    font-weight: 700;
    src: url('/fonts/onvan/fonts/ttf/onvan.ttf');
    src: url('/fonts/onvan/fonts/woff2/onvan.woff2') format('woff2'), url('/fonts/onvan/fonts/ttf/onvan.ttf') format('truetype');
  }

  @font-face {
    font-display: swap;
    font-family: Estedad;
    font-style: normal;
    font-weight: 700;
    src: url('/fonts/estedad/fonts/ttf/Estedad-Bold.ttf');
    src: url('/fonts/woff2/Estedad-Bold.woff2') format('woff2'), url('/fonts/ttf/Estedad-Bold.ttf') format('truetype');
  }
  @font-face {
    font-display: swap;
    font-family: Estedad;
    font-style: normal;
    font-weight: 500;
    src: url('/fonts/estedad/fonts/ttf/Estedad-Medium.ttf');
    src: url('/fonts/estedad/fonts/woff2/Estedad-Medium.woff2') format('woff2'),
      url('/fonts/estedad/fonts/ttf/Estedad-Medium.ttf') format('truetype');
  }
  @font-face {
    font-display: swap;
    font-family: Estedad;
    font-style: normal;
    font-weight: 300;
    src: url('/fonts/estedad/fonts/ttf/Estedad-Light.ttf');
    src: url('/fonts/estedad/fonts/woff2/Estedad-Light.woff2') format('woff2'), url('/fonts/estedad/fonts/ttf/Estedad-Light.ttf') format('truetype');
  }

  @font-face {
    font-display: swap;
    font-family: Estedad;
    font-style: normal;
    font-weight: 400;
    src: url('/fonts/estedad/fonts/ttf/Estedad-Regular.ttf');
    src: url('/fonts/estedad/fonts/woff2/Estedad-Regular.woff2') format('woff2'),
      url('/fonts/estedad/fonts/ttf/Estedad-Regular.ttf') format('truetype');
  }


  
</style>
