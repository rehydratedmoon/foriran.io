{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "removeComments": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@stores": ["src/stores/*"]
    },
    "types": ["vite-plugin-pwa/info"]
  }
}
